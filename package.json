{
  "name": "npm-from-past",
  "version": "1.0.2",
  "description": "Run npm/yarn commands as if you were in the past",
  "repository": {
    "type": "git",
    "url": "https://github.com/avin/npm-from-past.git"
  },
  "bin": {
    "npm-from-past": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "cross-env NPM_FROM_PAST_DEBUG=1 ts-node ./src/index.ts"
  },
  "keywords": [],
  "author": "a.vinogradov",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.6",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
